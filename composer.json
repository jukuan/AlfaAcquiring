{
    "name": "jukuan/alfa-acquiring",
    "description": "Internet Acquiring helper for AlfaBank (Belarus), non-official.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julian",
            "email": "i@juljan.by"
        }
    ],
    "keywords": [
        "Alfa-Bank",
        "internet acquiring"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "sort-packages": true,
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "AlfaAcquiring\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stripe\\": [
                "tests/"
            ]
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.10"
    }
}
