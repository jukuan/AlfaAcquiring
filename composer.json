{
    "name": "jukuan/alfa-acquiring",
    "description": "Internet Acquiring helper for AlfaBank (tested in Belarus only for now), non-official.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julian",
            "email": "i@juljan.by"
        }
    ],
    "keywords": [
        "Belarus",
        "Беларусь",
        "Alfa-Bank",
        "alfabank",
        "acquiring",
        "payment",
        "credit card"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "sort-packages": true,
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "AlfaAcquiring\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stripe\\": [
                "tests/"
            ]
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.12",
        "phpunit/phpunit": "^9.5"
    }
}
